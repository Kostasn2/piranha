node float => __piranha__float {
    input __in;
}

node float_to_string => __piranha__float_to_string {
    input __in [float];
    alias output __out [string];
}

node string => __piranha__string {
    input __in [string];
}

node test {
    input a [string]: 5;
    output b [string]: a;
}

test(5)
test("hello")
test(5.0)
test(10)
